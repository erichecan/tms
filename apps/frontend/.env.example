# TMS前端环境配置示例
# 复制此文件为 .env.local 用于本地开发

# API配置
VITE_API_BASE_URL=http://localhost:8000/api
VITE_APP_VERSION=1.0.0

# Google Maps API配置
# 申请地址：https://console.cloud.google.com/google/maps-apis/
VITE_GOOGLE_MAPS_API_KEY=your-frontend-maps-api-key-here

# 应用配置
VITE_APP_TITLE=TMS物流管理平台
VITE_APP_DESCRIPTION=智能物流运营管理系统

# 开发配置
VITE_DEV_SERVER_PORT=3000
VITE_DEV_SERVER_HOST=localhost

# 功能开关
VITE_ENABLE_MAPS=true
VITE_ENABLE_REAL_TIME_TRACKING=true
VITE_ENABLE_ROUTE_OPTIMIZATION=true

# 地图配置
VITE_MAPS_DEFAULT_CENTER_LAT=43.6532
VITE_MAPS_DEFAULT_CENTER_LNG=-79.3832
VITE_MAPS_DEFAULT_ZOOM=10
VITE_MAPS_LANGUAGE=en
VITE_MAPS_REGION=CA

# 物流业务配置
VITE_BASE_SHIPMENT_COST=80
VITE_DISTANCE_RATE_PER_KM=2.00
VITE_DRIVER_HOURLY_RATE=25